<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler">
<st:contentType value="text/html" />
<html>
  <body>
    <h1>Test OpenID client</h1>
    <j:if test="${it.openid!=null}">
      <div>
        Currently logged in as ${it.openid}
      </div>
    </j:if>
    <form action="start">
      OpenID: <input type="text" name="openid" size="100" />
      <br />
      <input type="submit" />
    </form>
  </body>
</html>
</j:jelly>
